GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Masonry" "${PODS_ROOT}/Headers/Public/PureLayout" "${PODS_ROOT}/Headers/Public/UpdateAutoLayoutConstraints"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Masonry" -isystem "${PODS_ROOT}/Headers/Public/PureLayout" -isystem "${PODS_ROOT}/Headers/Public/UpdateAutoLayoutConstraints"
OTHER_LDFLAGS = -ObjC -l"Pods-Masonry" -l"Pods-PureLayout" -l"Pods-UpdateAutoLayoutConstraints" -framework "Foundation" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods